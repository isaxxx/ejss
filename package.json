    "glob": "^7.1.2",
    "glob-base": "^0.3.0",
    "htmlhint": "^0.9.13",
    "minimize": "^2.2.0",
    "path": "^0.12.7",
    "update-notifier": "^2.3.0",
    "yargs": "^11.0.0"
